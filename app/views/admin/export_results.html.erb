<div class="row center-align flow-text">
    <div class="col s12">

        <div class="row">
            <div class="col s12">
                <h5>Admin: Export Weekly Results</h5>
            </div>
        </div>

        <div class="row">
            <div class="col s12">
                <% @weeks.each do |wk| %>
                    <%= link_to 'Week ' + wk.id.to_s, admin_export_results_path(format: :xlsx, id: wk.id)  %>
                    <br/>
                <% end %>
            </div>
        </div>

    </div>
</div>